set style line 4 lt 2 lw 4 lc 2
set style line 5 lt 1 lw 4 lc 9 pt 8
set style line 6 lt 6 lw 4 lc rgb "#2244ff"
set style line 7 lt 1 lw 6 lc 1
set style line 8 lt 2 lw 6 lc 3
set style line 9 lt 6 lw 6 lc 0
set style line 10 lt 1 lw 1 lc 9
